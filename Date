				genericLocators.datetextbox(driver,"Start Date", testCaseDataSd.get("SYSTEM_CLOSED").get(0)), CommonOperations.getDate("M/d/yyyy", testCaseDataSd.get("SYSTEM_CLOSED").get(0)), action);
